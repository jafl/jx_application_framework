# Useful directories

MYCODEDIR := .

# Directories to search for header files

SEARCHDIRS := -I${MYCODEDIR}

# makemake variables

LINKER       := gcc
DEPENDFLAGS  := -g -Wall -Werror ${SEARCHDIRS}

# make variables

CC        := gcc
CXX       := g++
CCC       := g++
FC        := f77
CFLAGS     = ${DEPENDFLAGS}
CXXFLAGS   = ${DEPENDFLAGS}
FFLAGS     = 
CPPFLAGS   = 
LOADLIBES := -lm
LDFLAGS   := 

%.o : %.cpp
	${CXX} ${CPPFLAGS} -c $< ${CXXFLAGS}

default: all

