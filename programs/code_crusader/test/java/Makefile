# hello_world

hello_world : ./code/HelloWorld.class ./code/test.class ./code/test2.class

# target for making everything

.PHONY : all
all: hello_world


# target for removing all object files

.PHONY : tidy
tidy::
	@${RM} core ./code/HelloWorld.class ./code/test.class ./code/test2.class

# target for removing all object files

.PHONY : clean
clean:: tidy
	@${RM} hello_world

# list of all source files

MM_ALL_SOURCES := ./code/HelloWorld.java ./code/test.java ./code/test2.java


# target for checking a source file

CHECKSYNTAXFILE := ${basename ${filter %${CHECKSTRING}, ${MM_ALL_SOURCES}}}

.PHONY : checksyntax
checksyntax:
  ifneq (${CHECKSYNTAXFILE},)
	@${MAKE} ${addsuffix .o, ${CHECKSYNTAXFILE}}
  else
	@echo No target to make ${CHECKSTRING}
  endif


# target for touching appropriate source files

.PHONY : touch
touch::
	@list=$$(grep -l ${TOUCHSTRING} ${MM_ALL_SOURCES}); \
     for file in $$list; do { echo $$file; touch $$file; } done


# target for calculating dependencies (MAKEMAKE)

.PHONY : jdepend
jdepend:
	@${MAKEMAKE} --depend Makefile -- ${DEPENDFLAGS} --  ./code/HelloWorld.java ./code/HelloWorld.class ./code/test.java ./code/test.class ./code/test2.java ./code/test2.class


