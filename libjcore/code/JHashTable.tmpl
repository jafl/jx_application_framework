/******************************************************************************
 Remove (protected)

 *****************************************************************************/

template <class V>
void
JHashTable<V>::Remove
	(
	const JSize index
	)
{
	JHashRecord<V>& thisRecord = itsArray[index];

	if ( thisRecord.IsFull() )
		{
		itsArray[index].Remove();
		--itsElementCount;
		if (itsElementCount == 0)
			{
			FitToLimits(0, kJTrue); // Force a resize to clear deleted records
			}
		else
			{
			FitToLimits();
			}
		}
}

