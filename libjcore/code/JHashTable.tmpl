template <class V>
void
JHashTable<V>::MarkEmpty
	(
	const JSize index
	)
{
	JHashRecord<V>& thisRecord = itsArray[index];

	JHashRecordT::State oldState = thisRecord.GetState();
	if (oldState != JHashRecordT::kEmpty)
		{
		itsArray[index].MarkEmpty();

		--itsLoadCount;
		if (oldState == JHashRecordT::kFull)
			{
			--itsElementCount;
			}
		FitToLimits();
		}
}

