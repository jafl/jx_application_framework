 Destructor

 *****************************************************************************/

template <class V>
JHashTable<V>::~JHashTable()
{
	delete[] itsArray;
	itsArray = NULL;
	itsMaxIndex = 0;

	delete itsCursor;
	itsCursor = NULL;
}

